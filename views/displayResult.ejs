<div class="display-result">
  <%= data %>
</div>
<script>
  let str = document.querySelector('.display-result').innerText;
  console.log(str);
  console.log(typeof(str));
  let arr = [];
  for(let i = 0;i<str.length;i++)
  {
    if(str.charAt(i) == '%')
    {
      arr.push(i);
    }
  }
  console.log(arr);
  document.querySelector('.display-result').innerHTML = "";
  for(let i = 0 ; i<arr.length;i++)
  {
    if(i == 0)
    {
      document.querySelector('.display-result').innerHTML += `
      <div>
        ${str.substr(0,arr[i]+1)}
      </div>
    `
    console.log(i);
    console.log(0,arr[i]+1);
    }
    else
    {
      document.querySelector('.display-result').innerHTML += `
      <div>
        ${str.substr(arr[i-1]+1,arr[i]+1)}
      </div>
    `
    console.log(i)

    console.log(arr[i-1]+1,arr[i]+1);
    }
  }
</script>